{
  "routes": [
    {
      "src": "/login",
      "dest": "/login.html"
    },
    {
      "src": "/profile",
      "dest": "/profile.html"
    },
    {
      "handle": "filesystem",
      "src": "/static/.*"
    },
    {
      "handle": "filesystem",
      "src": "/images/.*"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
